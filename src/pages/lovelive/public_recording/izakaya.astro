---
export const prerender = true;

import SpotListMap from '$/components/lovelive/public_recording/SpotListMap.astro';

const title = '牛久公録 飲み情報';
const description = 'イベント後に飲めるお店の情報です。';

const SPOT_LIST = ['keiji', 'tanuki', 'ohsho', 'miya', 'wakasama', 'hidakaya']
---

<SpotListMap
  title={title}
  description={description}
  spotList={SPOT_LIST}
/>
