---
export const prerender = true;

import SpotListMap from '$/components/lovelive/public_recording/SpotListMap.astro';

const title = '牛久公録 居酒屋情報';
const description = '会場周辺後にやっている居酒屋さんの情報です。';

const INCLUDE_SLUGS = ['central_center'];
---

<SpotListMap
  title={title}
  description={description}
  additionalKey="kouroku_izakaya"
  includeSlugs={INCLUDE_SLUGS}
/>

